<form action="/createplaces" method="POST">
  <div class="form-group">
    <label>Name</label>
    <input type="name" class="form-control"  name="name">
  </div>
  <div class="form-group">
    <label>Location</label>
    <input type="location" class="form-control" name="location">
  </div>
   <div class="form-group">
    <label for="exampleFormControlSelect1">Place type</label>
    <select class="form-control" name="placesType">
      <option>coffe shop</option>
      <option>bookstore</option>
    </select>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>